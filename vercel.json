{
  "functions": {
    "app/api/twilio/twiml/route.ts": {
      "maxDuration": 30
    },
    "app/api/twilio/status-callback/route.ts": {
      "maxDuration": 30
    },
    "app/api/twilio/initiate-call/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/twilio/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "*"
        }
      ]
    }
  ]
}
